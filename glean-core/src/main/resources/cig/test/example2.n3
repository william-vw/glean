prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix math: <http://www.w3.org/2000/10/swap/math#>
prefix state: <http://niche.cs.dal.ca/ns/state.owl#>
prefix cond: <http://niche.cs.dal.ca/ns/condition/base.owl#>
prefix gl: <http://niche.cs.dal.ca/ns/glean/base.owl#>
prefix cig: <http://niche.cs.dal.ca/ns/cig/cig.owl#>
prefix in: <http://niche.cs.dal.ca/ns/cig/input/fhir.owl#>
prefix fhir: <http://hl7.org/fhir/>
prefix : <http://example.org/>

#
:Example_Workflow2 a cig:MedicalWorkflow ;
	state:in gl:Inactive ;
	rdfs:label "Example Workflow 2" ;
	rdfs:comment """Example Workflow 2""" ;

	cig:inputFile "/cig/test/input/example2.n3" ;

	gl:root false ;
	gl:subTask :task21 , :task22 , :task23 , :task24 , :task25 .

#
:task21 a gl:DecisionTask ;
	state:in gl:Inactive ;
	gl:conditional false ; 

	rdfs:label "decision1?" ;
	rdfs:comment """Decision 1?""" ;
	
	cig:input :decision21_report ;
	
	gl:decisionBranch [
		state:in gl:Inactive ;
		gl:order 0 ;
		
		gl:precondition [
			rdfs:label "option1" ;
			rdfs:comment """Option 1""" ;
			
			cond:allOf (
				[ cond:premise { ?obs fhir:Observation.code :some_code2 ;
						fhir:Observation.valueBoolean true } ]
			)
		] ;
		
		gl:branchTarget :task22
		
	] , [
		state:in gl:Inactive ;
		gl:order 1 ;
		
		gl:precondition [ 
			rdfs:label "option2" ;
			rdfs:comment """Option 2""" ;
			
			cond:allOf (
				[ cond:premise { ?obs fhir:Observation.code :some_code2 ;
						fhir:Observation.valueBoolean false } ]
			)
		] ;
		
		gl:branchTarget :task23 
	] .

#
:task22 a gl:Task ;
	state:in gl:Inactive ;
	gl:conditional false ; 
	gl:involvesAction false ;
	
	gl:next :task24 ;
	
	rdfs:label "task1.1" ;
	rdfs:comment """Task 1.1""" .

#
:task23 a gl:Task ;
	state:in gl:Inactive ;
	gl:conditional false ; 
	gl:involvesAction false ;
	
	gl:next :task24 ;
	
	rdfs:label "task1.2" ;
	rdfs:comment """Task 1.2""" .

#
:task24 a gl:Task ;
	state:in gl:Inactive ;
	gl:conditional false ; 
	gl:involvesAction true ;
	
	cig:input :task_state_report ;

	gl:next :task25 ;
	
	rdfs:label "task2" ;
	rdfs:comment """Task 2""" .

#
:task25 a gl:EndPoint ;
	state:in gl:Inactive ;
	gl:conditional false ; 
	
	rdfs:label "endpoint" ;
	rdfs:comment "Endpoint" .