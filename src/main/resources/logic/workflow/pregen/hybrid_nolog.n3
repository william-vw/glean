        <http://niche.cs.dal.ca/ns/state.owl#in>
            <http://niche.cs.dal.ca/ns/glean/base.owl#Idle> .}  log:impliedBy {
            ?task
                <http://niche.cs.dal.ca/ns/state.owl#in>
                    <http://niche.cs.dal.ca/ns/glean/base.owl#Discarded> .        } ;
    log:impliedBy {
