		fhir:ObservationDefinition.category in:category_survey , in:category_present ;
		fhir:ObservationDefinition.permittedDataType in:dt_boolean ;
		fhir:ObservationDefinition.mutexWith (
			:code_no_cv_risk_factors
		)
	] .
	
