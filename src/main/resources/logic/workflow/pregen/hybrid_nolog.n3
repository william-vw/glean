            ?sub  <http://niche.cs.dal.ca/ns/state.owl#in>
                    <http://niche.cs.dal.ca/ns/glean/base.owl#Discarded> .        } .

{
    ?x  rdf:type ?c1 .
    ?c1  rdfs:subClassOf ?c2 .}  =>  {
    ?x  rdf:type ?c2 .} .

{
