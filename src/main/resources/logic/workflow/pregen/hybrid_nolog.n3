            "true"^^xsd:boolean .
    ?entity
        <http://niche.cs.dal.ca/ns/glean/base.owl#precondition>
            _:b1 .
    _:b1  <http://niche.cs.dal.ca/ns/condition/base.owl#conditionMet>
            "true"^^xsd:boolean .}  rdf:type log:StableTruth .
    ?entity
        <http://niche.cs.dal.ca/ns/state.owl#in>
